[autoload]

Testing="*res://addons/simple testing/scripts/testing.gd"
TestUtils="*res://addons/simple testing/scripts/test_utils.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/simple testing/plugin.cfg")
